{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-list": "^1.7.3",
    "@umijs/preset-react": "1.x",
    "copy-to-clipboard": "^3.3.1",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.7.2",
    "markdown-it": "^12.0.6",
    "markdown-it-highlightjs": "^3.4.0",
    "rc-queue-anim": "^1.8.5",
    "react-markdown-editor-lite": "^1.2.4",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "umi": "^3.4.19",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.4.19",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
